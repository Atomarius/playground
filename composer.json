{
  "name": "elsuperbeano/playground",
  "description": "Just to play around",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "ElSuperbeano",
      "email": "marius.schuette@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.zendframework.com/"
    }
  ],
  "autoload": {
    "psr-0": { "DDD\\": "src/", "Profiler\\": "src/" }
  },
  "autoload-dev": {
    "psr-0": { "DDD\\": "tests/", "\\Profiler": "tests/" }
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "twbs/bootstrap": "^3.3"
  },
  "scripts": {
    "post-install-cmd": [
      "chmod +x ./scripts/test.php"
    ]
  },
  "require": {
    "zendframework/zendframework1": "~1.10",
    "predis/predis": "~1.0.0"
  }
}
